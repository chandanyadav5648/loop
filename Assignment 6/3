// sum of first n odd natural numbers
#include<stdio.h>
int main()
{
    int n , sum=0;
    printf("enter n number to calculate sum of first n odd natural numbers:");
    scanf("%d",&n);

    for(int i=1 ; i<=2*n ; i++)
          {
              if(i%2!=0)
                sum = sum + i;

          }

     printf("%d",sum);

     return 0;
}

