//sum of square of first n natural numbers
#include<stdio.h>
int main()
{
    int n , sum=0;
    printf("enter n number to calculate sum of square of first n natural numbers:");
    scanf("%d",&n);

    for(int i=1 ; i<=n ; i++)
          {
              sum = sum + i*i;
          }

       printf("%d",sum);

       return 0;


}
